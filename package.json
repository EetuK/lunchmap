{
	"name": "my-app",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev --host",
		"build": "vite build",
		"preview": "vite preview",
		"test": "playwright test",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"test:unit": "vitest",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write .",
		"update-data": "docker build -t table-converter ./scripts && docker run -v $(pwd)/src/data:/data -e SHEETS_URL=https://docs.google.com/spreadsheets/d/e/2PACX-1vRGrBHSKwjzZm0EndAP8aBecxh3v2tCJbVzFL54d7Lb96mhLhV9u_2BbAGRmKBU9AuHkdUqGJq12m3x/pubhtml# --rm table-converter  --remove-orphans"
	},
	"devDependencies": {
		"@beyonk/svelte-mapbox": "^8.2.0",
		"@playwright/test": "^1.28.1",
		"@sveltejs/adapter-auto": "next",
		"@sveltejs/kit": "next",
		"@types/mapbox-gl": "^2.7.10",
		"@types/svelte-leafletjs": "^0.8.2",
		"@typescript-eslint/eslint-plugin": "^5.45.0",
		"@typescript-eslint/parser": "^5.45.0",
		"eslint": "^8.28.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"leaflet": "^1.9.3",
		"mapbox-gl": "^2.11.1",
		"prettier": "^2.8.0",
		"prettier-plugin-svelte": "^2.8.1",
		"sass": "^1.56.2",
		"svelte": "^3.54.0",
		"svelte-check": "^2.9.2",
		"svelte-leafletjs": "^0.10.0",
		"svelte-local-storage-store": "^0.3.2",
		"svelte-multiselect": "^8.2.0",
		"svelte-pathfinder": "^3.4.0",
		"tslib": "^2.4.1",
		"vh-check": "^2.0.5",
		"vite": "^4.0.0",
		"vite-plugin-iso-import": "^1.0.0",
		"vitest": "^0.25.3"
	},
	"type": "module"
}
